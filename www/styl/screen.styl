@import colors
@import prefixes
body
    margin 0px
    font-family arial, sans-serif
    #content
        position absolute
        top 0px
        bottom 0px
        width 100%
        overflow hidden
    svg
        background HNBlue
        .land
            fill white
        .boundary
            fill none
            stroke #333
            stroke-width 0.5px
        .country
            stroke none
            &:hover
                opacity 0.7
